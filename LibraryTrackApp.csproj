    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\add-file.png" />
    <None Remove="images\book.png" />
    <None Remove="images\icons\file-yellow.png" />
